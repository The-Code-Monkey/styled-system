{
  "version": "1.1.3",
  "license": "MIT",
  "name": "@bunstack/styled-system",
  "homepage": "https://github.com/The-Code-Monkey/styled-system",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Code-Monkey/TechStack.git"
  },
  "bugs": {
    "url": "https://github.com/The-Code-Monkey/TechStack/issues"
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tcm watch",
    "build": "tcm build",
    "test": "tcm test",
    "lint": "tcm lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "author": "The-Code-Monkey",
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "10 KB"
    },
    {
      "path": "./src",
      "limit": "30 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/file": "9.0.0",
    "@techstack/tcm-cli": "1.2.85",
    "@types/jest": "29.5.5",
    "jest": "29.7.0",
    "size-limit": "9.0.0",
    "styled-components": "6.0.7",
    "ts-jest": "29.1.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "@techstack/memoize": "1.2.85",
    "csstype": "3.1.2",
    "react": "18.2.0"
  },
  "gitHead": "ab9d2fe77834a101f8c744df53be2e4166f881cb",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "rules": {
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-explicit-any": "warn",
      "import/no-unresolved": "off",
      "import/no-named-as-default": "off",
      "import/no-anonymous-default-export": "off",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  }
}

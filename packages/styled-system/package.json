{
  "version": "1.0.42",
  "license": "MIT",
  "name": "@techstack/styled-system",
  "homepage": "https://github.com/The-Code-Monkey/TechStack/blob/dev/packages/styled-system/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Code-Monkey/TechStack.git"
  },
  "bugs": {
    "url": "https://github.com/The-Code-Monkey/TechStack/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/styled-system.esm.js",
  "exports": {
    "import": "./dist/styled-system.esm.js",
    "require": "./dist/index.js",
    "default": "./dist/styled-system.esm.js"
  },
  "typings": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tcm watch",
    "build": "tcm build",
    "test": "tcm test",
    "lint": "tcm lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "author": "The-Code-Monkey",
  "size-limit": [
    {
      "path": "dist/styled-system.esm.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/file": "8.2.6",
    "@techstack/tcm-cli": "0.4.233",
    "@types/jest": "29.5.2",
    "jest": "29.6.1",
    "size-limit": "8.2.6",
    "styled-components": "6.0.2",
    "ts-jest": "29.1.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.1",
    "@techstack/memoize": "0.4.142",
    "csstype": "3.1.2",
    "react": "18.2.0"
  },
  "gitHead": "7bb2bc7bbce0432773369ca533adf6d1719e877a",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "rules": {
      "@typescript-eslint/no-empty-function": "off",
      "import/no-unresolved": "off",
      "import/no-named-as-default": "off",
      "import/no-anonymous-default-export": "off",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  }
}
